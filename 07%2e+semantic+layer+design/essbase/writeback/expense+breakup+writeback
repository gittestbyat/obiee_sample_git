function updateExpense(strScenario, strCat, strQtr, iExp){
	var strQuery= &quot;UPDATE `F - Essbase Interaction`.`Basic` SET `Total Expenses` = &quot; + iExp + 
		&quot; WHERE `F - Essbase Interaction`.`Product`.`Category` = &#39; + strCat +
		&quot;&#39; AND `F - Essbase Interaction`.`Scenario`.`Scenarios` = &#39; + strScenario +
		&quot;&#39; AND `F - Essbase Interaction`.`Year`.`Quarter` = &#39; + strQtr + &quot;&#39;&quot;;
	strLSQL= strQuery.replace(/`/g,&quot;\&quot;&quot;);
	alert(strLSQL);
}
