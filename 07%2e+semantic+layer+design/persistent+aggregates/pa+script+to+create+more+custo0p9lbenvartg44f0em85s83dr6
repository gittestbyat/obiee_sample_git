&lt;pre&gt;
set variable LOGLEVEL=7: create aggregates 
/*===========================================================*/ 
[b]EXA_ESSB[/b] 
for &quot;01 - Sample App Exa&quot;.&quot;F0 Sales Base Measures&quot; ( 
&quot;1- Revenue&quot;,
&quot;2- Billed Quantity&quot;,
&quot;3- Discount Amount&quot;, 
&quot;11- Fixed Costs&quot;
)    
[b]at levels[/b] (
&quot;01 - Sample App Exa&quot;.&quot;H1 Products&quot;.&quot;Products Detail&quot;, 
&quot;01 - Sample App Exa&quot;.&quot;H3 Offices&quot;.&quot;Offices Detail&quot;, 
&quot;01 - Sample App Exa&quot;.&quot;H0 Time&quot;.&quot;Month&quot; 
) 
[b]using connection pool[/b] &quot;02 - Sample App Exa Data (ESSB)&quot;.&quot;Aggr Connection&quot; 
[b]in[/b] &quot;02 - Sample App Exa Data (ESSB)&quot;..&quot;EXA_ESSB&quot;;
&lt;/pre&gt;
