ng></saw:column></saw:columns><saw:sql>Select 
S.Sid,S.Machine,S.Osuser,S.Status,S.Program, S.Module, 
S.Client_Info,S.State,S.Schemaname,
Ar.First_Load_Time,To_Char(Ar.Last_Active_Time, &#39;YY-MM-DD-HH24:MI:SS&#39;) As &quot;last active&quot;,
Ar.Elapsed_Time/1000000 As &quot;Elapsed Sec&quot;,Ar.Cpu_Time/1000000 As &quot;CPU Sec&quot;,
Ar.Application_Wait_Time/1000000 As &quot;Wait Sec&quot;,Ar.Plsql_Exec_Time/1000000 As &quot;PLSQL Exec Sec&quot;,
Ar.Px_Servers_Executions As &quot;PX Servers&quot;, 
&#39;&lt;div style=&quot;height:60px; width:300px; overflow:auto;&quot;&gt;&#39;||Ar.Sql_Text||&#39;&lt;/div&gt;&#39;
From   V$session S
       ,V$sqlarea Ar
Where S.Sql_Hash_Value = Ar.Hash_Value
And    S.Sql_Address = Ar.Address
Order By Ar.First_Load_Time Desc </saw:sql></saw:criteria><saw:views currentView="2"><saw:view xsi:type="saw:compoundView" name="compoundView!1"><saw:cvTable><saw:cvRow><saw:cvCell viewName="tableView!1"><saw:displayFormat><saw:formatSpec/></saw:displayFormat></saw:cvCell></saw:cvRow></saw:cvTable></saw:view><saw:view xsi:type="saw:titleView" name="titleView!1"/><saw:view xsi:type="saw:tableView" name="tableView!1" autoPreview="true"><saw:edges><saw:edge axis="page" showColumnHeader="true"/><saw:edge axis="section"/><saw:edge axis="row" showColumnHeader="true"><saw:edgeLayers><saw:edgeLayer type="column" columnID="c1" uiTableName=""/><saw:edgeLayer type="column" columnID="c2" uiTableName=""/><saw:edgeLayer type="column" columnID="c0" uiTableName=""/><saw:edgeLayer type="column" columnID="c4" uiTableName=""><saw:memberFormat><saw:displayFormat><saw:formatSpec vAlign="middle" wrapText="true"/></saw:displayFormat></saw:memberFormat></saw:edgeLayer><saw:edgeLayer type="column" columnID="c3" uiTableName=""/><saw:edgeLayer type="column" columnID="c7" uiTableName=""><saw:memberFormat><saw:displayFormat><saw:formatSpec vAlign="middle" wrapText="true"/></saw:displayFormat></saw:memberFormat></saw:edgeLayer><saw:edgeLayer type="column" columnID="c8" uiTableName=""><saw:memberFormat><saw:displayFormat><saw:formatSpec vAlign="middle" wrapText="true"/></saw:displayFormat></saw:memberFormat></saw:edgeLayer><saw:edgeLayer type="column" columnID="c9" uiTableName=""><saw:memberFormat><saw:displayFormat><saw:formatSpec vAlign="middle" wrapText="true"/></saw:displayFormat></saw:memberFormat></saw:edgeLayer><saw:edgeLayer type="column" columnID="c10" uiTableName=""><saw:memberFormat><saw:displayFormat><saw:formatSpec vAlign="middle" wrapText="true"/></saw:displayFormat></saw:memberFormat></saw:edgeLayer><saw:edgeLayer type="column" columnID="c11" visibility="visible" uiTableName=""><saw:memberFormat><saw:displayFormat><saw:formatSpec vAlign="middle" wrapText="true"/></saw:displayFormat