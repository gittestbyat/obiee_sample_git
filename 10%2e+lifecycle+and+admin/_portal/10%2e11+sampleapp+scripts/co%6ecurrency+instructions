	<sawd:dashboardColumn name="Column 0" duid="94apgmot0sbao4qn" canFreeze="false" frozen="false" layoutType="specificSize"><sawd:dashboardSection name="Section 17" duid="onmrj50qvftd2q2j" showSectionTitle="false" collapsible="false" horizontalLayout="true" layoutType="bestFit"><saw:displayFormat><saw:formatSpec borderPosition="none" wrapText="true" borderColor="#CCCCCC" borderStyle="single" paddingLeft="0" paddingRight="0" paddingTop="0" paddingBottom="0"/></saw:displayFormat><sawd:htmlView name="HTML 0" duid="to5r0gq9f1vqa4br"><sawd:HTML fmt="html">[b]&lt;font size=3 align=left color=#336666&gt;Sample Concurrency Simulator - Purpose and Usage Instructions&lt;/font&gt;[/b]</sawd:HTML></sawd:htmlView><sawd:linkView name="Link 0" target="_self" type="report" destination="/shared/01. QuickStart/_portal/1.00 General Index" layout="captionRight" duid="730rmpdau4u48g7l">
				<sawd:linkCaption>
