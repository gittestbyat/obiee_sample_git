create aggregates &quot;ag_Facts_Hive_Revenue&quot;[br/]
 for [br/]
&amp;nbsp;&amp;nbsp;&amp;nbsp;&quot;12 - Big Data Movie&quot;.&quot;Facts (Hive)&quot;(&quot;Price&quot;)[br/]
 at levels ([br/]
&amp;nbsp;&amp;nbsp;&amp;nbsp;&quot;12 - Big Data Movie&quot;.&quot;H2 Customer&quot;.&quot;Country&quot;, [br/]
&amp;nbsp;&amp;nbsp;&amp;nbsp;&quot;12 - Big Data Movie&quot;.&quot;H1 Movie&quot;.&quot;Genre&quot;, [br/]
&amp;nbsp;&amp;nbsp;&amp;nbsp;&quot;12 - Big Data Movie&quot;.&quot;H0 Time&quot;.&quot;Week&quot;)[br/]
 using connection pool [br/]
