    </sawd:dashboardSection><sawd:dashboardSection name="Section 7" horizontalLayout="true" collapsible="false" duid="kf2c31j7ima8n185" showSectionTitle="false" layoutType="bestFit"><saw:displayFormat><saw:formatSpec borderPosition="none" wrapText="true"/></saw:displayFormat><sawd:reportView name="Report 5" display="embed" showView="" duid="31eccs11d4qnvuvj"><saw:caption><saw:text>Refresh_Tagged</saw:text></saw:caption><sawd:reportRef type="catalog" path="/shared/09. Integrations and Customizations/Custom Interactions/Auto Refresh/Refresh_Tagged"/></sawd:reportView><sawd:reportView name="Report 4" display="embed" showView="" duid="a2os3n33qf7nvs86"><saw:caption><saw:text>Not_Refresh_Tagged</saw:text></saw:caption><sawd:reportRef type="catalog" path="/shared/09. Integrations and Customizations/Custom Interactions/Auto Refresh/Not_Refresh_Tagged"/></sawd:reportView></sawd:dashboardSection><sawd:dashboardSection name="Section 10" horizontalLayout="true" collapsible="false" duid="monh3pmempvp91qk" showSectionTitle="false" layoutType="bestFit"><saw:displayFormat><saw:formatSpec borderPosition="none" wrapText="true" backgroundColor="#FFFF99"/></saw:displayFormat><sawd:htmlView name="HTML 7" duid="e33uh2b9eed1kq8v"><sawd:HTML fmt="html">&lt;script type=&quot;text/javascript&quot;&gt;
var _setIntId1;

function setAutoRefresh(myObj){
	var refreshObj, intervalObj, refInterval, indexOfId, refreshFunction;
	
	refreshObj = document.getElementById(&#39;_autoref1&#39;);
	if(!refreshObj) return;
	
	intervalObj = myObj.elements[&quot;refreshint&quot;];
	refInterval = intervalObj.options[intervalObj.selectedIndex].value;
	indexOfId=-1;
	
	while ( !refreshObj.parentNode.getAttribute(&#39;vid&#39;))       
		refreshObj = refreshObj.parentNode;
	refreshID=refreshObj.parentNode.getAttribute(&#39;vid&#39;);
	indexOfId=refreshID.indexOf(&#39;~v:&#39;);
	if (indexOfId&gt;-1)     
		refreshID = refreshID.substring(0,indexOfId);
	refFunction = &quot;HereLink(&#39;&quot; + refreshID + &quot;&#39;,&#39;Refresh&#39;)&quot;;
	//alert(&#39;Setting Refresh &#39;+refFunction+&#39; - &#39;+refInterval);
	_setIntId1=setInterval(refFunction,1000*refInterval);
}

function handleRefSel(formObj){
	if (formObj.elements[&quot;cb&quot;].checked){
		_setIntId1=clearInterval(_setIntId1);
		setAutoRefresh(formObj);
	} else{
		alert(&#39;Refresh is now disabled. Select Auto Refresh check box to enable&#39;);
		_setIntId1=clearInterval(_setIntId1);
	}
}
&lt;/script&gt;

&lt;form&gt;
&lt;input  id=&quot;cb&quot; type=&quot;checkbox&quot; onChange=&quot;handleRefSel(this.parentNode)&quot;/&gt;Auto Refresh in 
&lt;select id=&quot;refreshint&quot; onChange=&quot;handleRefSel(this.parentNode)&quot;&gt;
&lt;option value=&quot;5&quot;&gt;5&lt;/option&gt;
&lt;option value=&quot;10&quot;&gt;10&lt;/option&gt;
&lt;option value=&quot;20&quot; selected=&quot;selected&quot;&gt;20&lt;/option&gt;
&lt;option value=&quot;30&quot;&gt;30&lt;/option&gt;
&lt;option value=&quot;60&quot;&gt;60&lt;/option&gt;
&lt;/select&gt;
seconds
&lt;/form&gt;

