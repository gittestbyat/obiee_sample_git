 SELECT Time.&quot;T02 Per Name Month&quot;,[br/] &quot;Base Facts&quot;.&quot;1 - Revenue&quot;  [br/]
 FROM &quot;A - Sample Sales&quot; [br/]
 WHERE &quot;Products&quot;.&quot;P2  Product Type&quot; = &#39;@{p1}&#39; [br/]
 AND &quot;Time&quot;.&quot;T05 Per Name Year&quot; = &#39;@{p2}&#39; [br/]
 ORDER BY 1[br/]
[br/] NOTE: p1 and p2 are contextual params from the table</sawd:HTML></sawd:htmlView></sawd:dashboardSection><saw:displayFormat><saw:formatSpec wrapText="true"/></saw:displayFormat></sawd:dashboardColumn></sawd:dashboardPage>