function tog_selection () {
	var namecnt=0;
        repnames = document.getElementsByName(&#39;scrnames&#39;);

	if (repnames.length&gt;0){
         	chkbox_status = !chkbox_status;
                for(var i = 0; i &lt; repnames.length; i++){
