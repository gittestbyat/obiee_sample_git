var dbpage=&#39;@{dashboard.currentPage}&#39;;

function namesel_selfnav(event) {
	var namelist = &quot;&quot;;
        repnames = document.getElementsByName(&#39;scrnames&#39;);

	if(repnames) {
		for(var i = 0; i &lt;repnames.length; i++){
			if (!repnames[i].checked) continue;
			if (namelist!=&quot;&quot;) namelist += &quot;,&quot;;
			namelist += &quot;&#39;&quot;+ repnames[i].value + &quot;&#39;&quot;;
		}
	}
	if(namelist==&quot;&quot;) 
		alert (&#39;Please select at least one name&#39;);
	else
		PortalPageNav(event, dbpath, dbpage, &#39;Rep&#39;, &#39;Name&#39;, namelist);
	return true;
}
