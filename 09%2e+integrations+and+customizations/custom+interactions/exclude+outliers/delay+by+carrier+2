p/customjs/SAutilsv1.3.js&#39;&gt;&lt;/script&gt;

&lt;script&gt;
function handleSelection(aParams){
   var v = aParams[&quot;p1&quot;];
   var cond = aParams[&quot;p2&quot;];

   if (cond==&quot;above&quot;)   
      exclusion_limit.high = v;
   else 
      exclusion_limit.low = v;
   
   exclusion_criteria = &#39;1=2&#39;;
   if (exclusion_limit.high)
	  exclusion_criteria += &#39; OR &quot;Arrival Delay&quot;.&quot;Avg Arr Delay (Min)&quot; &amp;gt;= &#39; + exclusion_limit.high;
   if (exclusion_limit.low)
	  exclusion_criteria += &#39; OR &quot;Arrival Delay&quot;.&quot;Avg Arr Delay (Min)&quot; &amp;lt;=  &#39; + exclusion_limit.low;
	
     SAutils.setPSVarPrompt(&#39;selection_criteria&#39;, exclusion_criteria);
}

function clearSelection(){
   exclusion_limit = {};
   SAutils.setPSVarPrompt(&#39;selection_criteria&#39;, &#39;1=2&#39;);
}
&lt;/script&gt;


