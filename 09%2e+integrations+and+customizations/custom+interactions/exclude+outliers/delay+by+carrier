&lt;script&gt;
function handleSelection(aParams){
   var c = &quot;&#39;&quot;+ aParams[&quot;p1&quot;] +&quot;&#39;&quot;;
   var idx = carriersel.indexOf(c);
   if (idx != -1)    carriersel.splice(idx, 1);
   else     carriersel.push(c);
  SAutils.setPSVarPrompt(&#39;carrier_selection&#39;, carriersel.toString());
}

function clearSelection(){
   carriersel=[];
   SAutils.setPSVarPrompt(&#39;carrier_selection&#39;, &#39;&#39;);
}
&lt;/script&gt;


