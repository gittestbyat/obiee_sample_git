      <dataSet name="ORData" type="complex">
         <sql>
            <![CDATA[select value
from   table(rqTableEval( cursor(select :uniqueCarrier UNIQUE_CARRIERS,
                                        :delayType DELAY_TYPE,
                                        :startYear START_YEAR,
                                        :endYear END_YEAR,
                                        :newDataStartYear NEW_DATA_START_YEAR,
                                        :newDataEndYear NEW_DATA_END_YEAR,
                                        :graphTypes GRAPH_TYPES from dual),cursor(select 1 "ore.connect" from dual),'XML','QCC-1'))]]>
         </sql>
      </dataSet>
   </dataSets>
   <output uniqueRowName="false">
