        <saw:caption fmt="html" oldID="kcap12387046_42948" captionID="kcap12851338_1682">
          <saw:text>&lt;br/&gt;&lt;table width=370 cellpadding=8 cellspacing=0 bgcolor=&quot;#F9F9F9&quot; style=&quot;border: 1px solid #D0D0E0&quot;&gt;&lt;tr&gt;&lt;td&gt;&lt;font color=#666699 size=2&gt;&lt;b&gt;&lt;i&gt;Contents :&lt;/i&gt;&lt;/font&gt;&lt;br/&gt;&lt;br/&gt;&lt;img src=&quot;res/sk_blafp/common/rightarrow.gif&quot;&gt;&lt;font color=#000080 size=2&gt;&amp;nbsp;1. Purpose and Usage&lt;/b&gt;&lt;/font&gt;&lt;br/&gt;&amp;nbsp;&amp;nbsp;&amp;nbsp;&lt;img src=&quot;res/sk_blafp/b_mozilla_4/blocked.gif&quot;&gt;&lt;font color=#666699&gt;&lt;b&gt;&amp;nbsp;1.1 Purpose &lt;/b&gt;&lt;/font&gt;&lt;br/&gt;&amp;nbsp;&amp;nbsp;&amp;nbsp;&lt;img src=&quot;res/sk_blafp/b_mozilla_4/blocked.gif&quot;&gt;&lt;font color=#666699&gt;&lt;b&gt;&amp;nbsp;1.2 Usage &lt;/b&gt;&lt;/font&gt;&lt;br/&gt;&lt;br/&gt;&lt;img src=&quot;res/sk_blafp/common/rightarrow.gif&quot;&gt;&lt;font color=#000080 size=2&gt;&lt;b&gt;&amp;nbsp;2. Layout&lt;/b&gt;&lt;/font&gt;&lt;br/&gt;&amp;nbsp;&amp;nbsp;&amp;nbsp;&lt;img src=&quot;res/sk_blafp/b_mozilla_4/blocked.gif&quot;&gt;&lt;font color=#666699&gt;&lt;b&gt;&amp;nbsp;2.1 Views Layout &lt;/b&gt;&lt;/font&gt;&lt;br/&gt;&amp;nbsp;&amp;nbsp;&amp;nbsp;&lt;img src=&quot;res/sk_blafp/b_mozilla_4/blocked.gif&quot;&gt;&lt;font color=#666699&gt;&lt;b&gt;&amp;nbsp;2.2 Navigations &lt;/b&gt;&lt;/font&gt;&lt;br/&gt;&lt;br/&gt;&lt;img src=&quot;res/sk_blafp/common/rightarrow.gif&quot;&gt;&lt;font color=#000080 size=2&gt;&lt;b&gt;&amp;nbsp;3. Technical Design&lt;/b&gt;&lt;/font&gt;&lt;br/&gt;&amp;nbsp;&amp;nbsp;&amp;nbsp;&lt;img src=&quot;res/sk_blafp/b_mozilla_4/blocked.gif&quot;&gt;&lt;font color=#666699&gt;&lt;b&gt;&amp;nbsp;3.1 RPD Columns Required &lt;/b&gt;&lt;/font&gt;&lt;br/&gt;&amp;nbsp;&amp;nbsp;&amp;nbsp;&lt;img src=&quot;res/sk_blafp/b_mozilla_4/blocked.gif&quot;&gt;&lt;font color=#666699&gt;&lt;b&gt;&amp;nbsp;3.2 Specific Filters &lt;/b&gt;&lt;/font&gt;&lt;br/&gt;&amp;nbsp;&amp;nbsp;&amp;nbsp;&lt;img src=&quot;res/sk_blafp/b_mozilla_4/blocked.gif&quot;&gt;&lt;font color=#666699&gt;&lt;b&gt;&amp;nbsp;3.3 Presentation Variables &lt;/b&gt;&lt;/font&gt;&lt;br/&gt;&amp;nbsp;&amp;nbsp;&amp;nbsp;&lt;img src=&quot;res/sk_blafp/b_mozilla_4/blocked.gif&quot;&gt;&lt;font color=#666699&gt;&lt;b&gt;&amp;nbsp;3.4 Top Page Prompts and Page Filter Prompts &lt;/b&gt;&lt;/font&gt;&lt;br/&gt;&amp;nbsp;&amp;nbsp;&amp;nbsp;&lt;img src=&quot;res/sk_blafp/b_mozilla_4/blocked.gif&quot;&gt;&lt;font color=#666699&gt;&lt;b&gt;&amp;nbsp;3.6 Limitations&lt;/b&gt;&lt;/font&gt;&lt;br/&gt;&lt;br/&gt;&lt;img src=&quot;res/sk_blafp/common/rightarrow.gif&quot;&gt;&lt;font color=#000080 size=2&gt;&lt;b&gt;&amp;nbsp;4. Leverage and Deploy&lt;/b&gt;&lt;/font&gt;&lt;br/&gt;&amp;nbsp;&amp;nbsp;&amp;nbsp;&lt;img src=&quot;res/sk_blafp/b_mozilla_4/blocked.gif&quot;&gt;&lt;font color=#666699&gt;&lt;b&gt;&amp;nbsp;4.1 Deploying A - Sample Sales reports in other environments &lt;/b&gt;&lt;/font&gt;&lt;br/&gt;&amp;nbsp;&amp;nbsp;&amp;nbsp;&lt;img src=&quot;res/sk_blafp/b_mozilla_4/blocked.gif&quot;&gt;&lt;font color=#666699&gt;&lt;b&gt;&amp;nbsp;4.2 Other Adaptations &lt;/b&gt;&lt;/font&gt;&lt;/td&gt;&lt;/tr&gt;&lt;/table&gt;</saw:text>
