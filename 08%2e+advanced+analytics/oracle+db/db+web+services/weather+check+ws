function displayWSResults() {
	xmlstring=document.getElementById(&#39;_wsresults&#39;).innerHTML;
	xmlstring=xmlstring.replace(/&amp;lt;/g,&#39;&lt;&#39;).replace(/&amp;gt;/g,&#39;&gt;&#39;);
	xmlstring=xmlstring.substring(xmlstring.indexOf(&#39;&lt;CurrentWeather&gt;&#39;),xmlstring.indexOf(&#39;&lt;/CurrentWeather&gt;&#39;)+17);
	xmlDoc=loadXMLString(xmlstring);

	wOut = &quot;&lt;br/&gt;&lt;table border=0&gt;&quot;;
	cols = xmlDoc.getElementsByTagName(&quot;CurrentWeather&quot;)[0].childNodes;
	for (j=0; j&lt;cols.length; j++) {
               if (cols[j].tagName )
		wOut += &quot;&lt;tr&gt;&lt;td&gt;&lt;b&gt;&quot; + cols[j].tagName + &quot;&lt;/b&gt;&lt;/td&gt;&lt;td&gt;&quot; + cols[j].textContent + &quot;&lt;/td&gt;&lt;/tr&gt;&quot;;
	}
        wOut += &quot;&lt;/table&gt;&quot;;
    document.getElementById(&#39;_wsresultsout&#39;).innerHTML = wOut;
}


function loadXMLString(txt) {
	if (window.DOMParser) {
	  parser=new DOMParser();
	  xmlDoc=parser.parseFromString(txt,&quot;text/xml&quot;);
	  }
	else /*IE*/ {
	  xmlDoc=new ActiveXObject(&quot;Microsoft.XMLDOM&quot;);
	  xmlDoc.async=false;
	  xmlDoc.loadXML(txt); 
	  }
	return xmlDoc;
}

displayWSResults();
