            <saw:caption fmt="html">
               <saw:text>&lt;div id=&quot;wordcloud&quot; /&gt;

&lt;script src=&quot;/analyticsRes/d3-cloud/lib/d3/d3.js&quot;&gt;&lt;/script&gt;
&lt;script src=&quot;/analyticsRes/d3-cloud/d3.layout.cloud.js&quot;&gt;&lt;/script&gt;
&lt;body&gt;
&lt;script&gt;
myWords=[];</saw:text></saw:caption></saw:prefix>
         <saw:postfix>
            <saw:caption fmt="html">
               <saw:text>w=@{Width}{900}; 
h=@{Height}{400};

/* ~~(Math.random() * 2) * 90 */

d3.layout.cloud().size([w,h])
.words(myWords)
      .rotate(function() { return 0; })
      .fontSize(function(d) { return d.size; })
      .font(&quot;Impact&quot;)
      .padding(1)
      .on(&quot;end&quot;, draw)
      .start();

  function draw(words) {
    d3.select(&quot;#wordcloud&quot;).append(&quot;svg&quot;)
        .attr(&quot;width&quot;, w)
        .attr(&quot;height&quot;, h)
      .append(&quot;g&quot;)
        .attr(&quot;transform&quot;, &quot;translate(&quot;+w/2+&quot;,&quot;+h/2+&quot;)&quot;)
      .selectAll(&quot;text&quot;)
        .data(words)
      .enter().append(&quot;text&quot;)
        .style(&quot;font-size&quot;, function(d) { return d.size + &quot;px&quot;; })
        .style(&quot;fill&quot;,&quot;@{TextColor}{steelblue}&quot;)
        .attr(&quot;text-anchor&quot;, &quot;middle&quot;)
        .attr(&quot;transform&quot;, function(d) {
          return &quot;translate(&quot; + [d.x, d.y] + &quot;)rotate(&quot; + d.rotate + &quot;)&quot;;
        })
        .text(function(d) { return d.text; });
  }
&lt;/script&gt;</saw:text></saw:caption></saw:postfix>
