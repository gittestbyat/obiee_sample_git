 f.RECORDID,
 ENDECA.MARKUPTEXT(&#39;@{contains_expr}{sienna AND van NOT accelerating}&#39;,f.RECORDID,&#39;DIM_CLAIM_COMMENTS_IDX1&#39;,&#39;&lt;font color=red&gt;&#39;,&#39;&lt;/font&gt;&#39;) COMPLAINT_DESCRIPTION,
 CLAIM_AGE_BUCKET,
 CLAIM_DEALER,
 CLAIM_MONTH,
 CLAIM_PERIOD,
 CLAIM_QUARTER,
 CLAIM_YEAR,
 DAYS_IN_SERVICE,
 CLAIM_AGE,
 CLAIM_TOT_GLOBL_AMT,
 SUPPLIER_ULTIMATENAME,
 SCORE(1) SCORE,
REGEXP_COUNT(COMPLAINT_DESCRIPTION, &#39;[^[:space:]]+&#39;) Number_Of_Words
FROM
 ENDECA.DIM_CLAIM_COMMENTS d,
 ENDECA.FACT_CLAIM f
WHERE
 f.RECORDID = d.RECORDID AND
 CONTAINS(COMPLAINT_DESCRIPTION, &#39;@{contains_expr}{sienna AND van NOT accelerating}&#39;, 1) &gt; 0
ORDER BY SCORE(1) DESC</saw:sql></saw:criteria><saw:views currentView="0"><saw:view xsi:type="saw:compoundView" name="compoundView!1"><saw:cvTable><saw:cvRow><saw:cvCell viewName="pivotTableView!1"><saw:displayFormat><saw:formatSpec hAlign="left"/></saw:displayFormat></saw:cvCell><saw:cvCell viewName="dvtchart!1"/></saw:cvRow><saw:cvRow><saw:cvCell viewName="tableView!1" colSpan="2"><saw:displayFormat><saw:formatSpec hAlign="left"/></saw:displayFormat></saw:cvCell></saw:cvRow></saw:cvTable></saw:view><saw:view xsi:type="saw:titleView" name="titleView!1"/><saw:view xsi:type="saw:tableView" name="tableView!1" autoPreview="true" width="900" height="700" rowsPerPage="20"><saw:edges><saw:edge axis="page" showColumnHeader="true"/><saw:edge axis="section"/><saw:edge axis="row" showColumnHeader="true"><saw:edgeLayers><saw:edgeLayer type="column" columnID="c0"><saw:headerFormat><saw:displayFormat><saw:formatSpec wrapText="true"/></saw:displayFormat><saw:caption><saw:text>Record ID</saw:text></saw:caption></saw:headerFormat><saw:memberFormat><saw:displayFormat><saw:formatSpec vAlign="middle" wrapText="true"/></saw:displayFormat></saw:memberFormat></saw:edgeLayer><saw:edgeLayer type="column" columnID="c13"><saw:memberFormat><saw:displayFormat><saw:formatSpec vAlign="middle" wrapText="true" fontColor="#336633"/></saw:displayFormat></saw:memberFormat><saw:headerFormat><saw:displayFormat><saw:formatSpec wrapText="true"/></saw:displayFormat><saw:caption><saw:text>Number Of Words</saw:text></saw:caption></saw:headerFormat></saw:edgeLayer><saw:edgeLayer type="column" columnID="c12"><saw:memberFormat><saw:displayFormat><saw:formatSpec vAlign="middle" wrapText="true" fontColor="#663366"/></saw:displayFormat></saw:memberFormat><saw:headerFormat><saw:displayFormat><saw:formatSpec wrapText="true"/></saw:displayFormat><saw:caption><saw:text>Search Score</saw:text></saw:caption></saw:headerFormat></saw:edgeLayer><saw:edgeLayer type="column" columnID="c10"><saw:memberFormat><saw:displayForm