	f.RECORDID,
	COMPLAINT_DESCRIPTION,
	CLAIM_AGE_BUCKET,
	CLAIM_DEALER,
	CLAIM_MONTH,
	CLAIM_PERIOD,
	CLAIM_QUARTER,
	CLAIM_YEAR,
	DAYS_IN_SERVICE,
	CLAIM_AGE,
	CLAIM_LBR_OTHR_GLOBL_AMT,
	CLAIM_LBR_TOT_GLOBL_AMT,
	CLAIM_ODMTR_MILEAGE,
	CLAIM_PART_TOT_GLOBL_AMT,
	CLAIM_TOT_GLOBL_AMT,
	SUPPLIER_ULTIMATENAME
FROM
	ENDECA.DIM_CLAIM_COMMENTS d,
	ENDECA.FACT_CLAIM f
WHERE
	f.RECORDID = d.RECORDID AND
	CONTAINS(COMPLAINT_DESCRIPTION, &#39;@{contains_expr}{sienna AND van NOT accelerating}&#39;, 1) &gt; 0</saw:sql></saw:criteria><saw:views currentView="0"><saw:view xsi:type="saw:compoundView" name="compoundView!1"><saw:cvTable><saw:cvRow><saw:cvCell viewName="pivotTableView!1" colSpan="2"><saw:displayFormat><saw:formatSpec hAlign="left"/></saw:displayFormat></saw:cvCell></saw:cvRow><saw:cvRow><saw:cvCell viewName="tableView!1"><saw:displayFormat><saw:formatSpec hAlign="left"/></saw:displayFormat></saw:cvCell><saw:cvCell viewName="dvtchart!1"><saw:displayFormat><saw:formatSpec/></saw:displayFormat></saw:cvCell></saw:cvRow></saw:cvTable></saw:view><saw:view xsi:type="saw:titleView" name="titleView!1"/><saw:view xsi:type="saw:tableView" name="tableView!1" autoPreview="true" width="800" rowsPerPage="10"><saw:edges><saw:edge axis="page" showColumnHeader="true"/><saw:edge axis="section"/><saw:edge axis="row" showColumnHeader="true"><saw:edgeLayers><saw:edgeLayer type="column" columnID="c0"><saw:headerFormat><saw:displayFormat><saw:formatSpec wrapText="true"/></saw:displayFormat><saw:caption><saw:text>Record ID</saw:text></saw:caption></saw:headerFormat><saw:memberFormat><saw:displayFormat><saw:formatSpec/></saw:displayFormat></saw:memberFormat></saw:edgeLayer><saw:edgeLayer type="column" columnID="c14"><saw:headerFormat><saw:displayFormat><saw:formatSpec wrapText="true"/></saw:displayFormat><saw:caption><saw:text>Claim Amount</saw:text></saw:caption></saw:headerFormat></saw:edgeLayer><saw:edgeLayer type="column" columnID="c3"><saw:headerFormat><saw:displayFormat><saw:formatSpec wrapText="true"/></saw:displayFormat><saw:caption><saw:text>Dealer</saw:text></saw:caption></saw:headerFormat></saw:edgeLayer><saw:edgeLayer type="column" columnID="c2"><saw:headerFormat><saw:displayFormat><saw:formatSpec wrapText="true"/></saw:displayFormat><saw:caption><saw:text>Age Bucket</saw:text></saw:caption></saw:headerFormat></saw:edgeLayer><saw:edgeLayer type="column" columnID="c1"><saw: