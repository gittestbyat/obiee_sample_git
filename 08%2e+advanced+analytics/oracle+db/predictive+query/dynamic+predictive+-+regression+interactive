ORIGIN,DEST,FLIGHTDATE,MONTH,DAYOFWEEK,CARRIER,DEPTIMEBLK,FLIGHTNUM,@{flight_pred}{ARRDELAYMINUTES} AS ACTUAL_VALUE,

PREDICTION (FOR @{flight_pred}{ARRDELAYMINUTES} USING @{flightattr}{FLIGHTDATE,FLIGHTNUM,CARRIER,ORIGIN,DEST,MONTH,DEPTIMEBLK,DAYOFWEEK}) OVER () PREDICTION,

EXTRACT((PREDICTION_DETAILS (FOR @{flight_pred}{ARRDELAYMINUTES},NULL,5 ABS USING @{flightattr}{FLIGHTDATE,FLIGHTNUM,CARRIER,ORIGIN,DEST,MONTH,DEPTIMEBLK,DAYOFWEEK}) OVER ()),&#39;/Details/Attribute&#39;).GETSTRINGVAL() PRED_DETAILS
FROM PERFORMANCE

WHERE ORIGIN = &#39;@{origairport}{LAX}&#39; AND DEST = &#39;@{destairport}{BOS}&#39; and @{predicate}{1=1}
