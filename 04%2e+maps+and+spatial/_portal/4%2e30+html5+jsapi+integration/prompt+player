                                  &quot;Brand&quot;:[&#39;FunPod&#39;,&#39;HomeView&#39;]};
 var container=document.getElementById(&quot;pPlayer&quot;);
 var resPath=&quot;/analyticsRes/SampleApp/customjs/PromptPlayer&quot;;
