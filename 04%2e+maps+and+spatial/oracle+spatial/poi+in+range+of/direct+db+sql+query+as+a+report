where 
s.cat_id = d.cat_id and
d.name = &#39;@{CATEGORY}{PHARMACY}&#39; and
sdo_within_distance(geometry, 
geocode_address(&#39;@{ADDRESS}{747 Howard St, San Francisco, CA 94013, US}&#39;),
&#39;distance=@{DISTANCE}{2000} unit=m&#39;)=&#39;TRUE&#39;



