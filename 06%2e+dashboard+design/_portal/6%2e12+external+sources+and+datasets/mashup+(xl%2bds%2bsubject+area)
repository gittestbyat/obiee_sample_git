SELECT[br/]
&amp;nbsp;&amp;nbsp;&amp;nbsp;XSA(&#39;weblogic&#39;.&#39;Passenger Count by Location&#39;).&quot;data&quot;.&quot;State&quot; s_1,[br/]
&amp;nbsp;&amp;nbsp;&amp;nbsp;&quot;A - Sample Sales&quot;.&quot;Base Facts&quot;.&quot;1- Revenue&quot; s_2,[br/]
&amp;nbsp;&amp;nbsp;&amp;nbsp;NTILE(&quot;A - Sample Sales&quot;.&quot;Base Facts&quot;.&quot;1- Revenue&quot;,3) s_3,[br/]
&amp;nbsp;&amp;nbsp;&amp;nbsp;XSA(&#39;weblogic&#39;.&#39;Passenger Count by Location&#39;).&quot;data&quot;.&quot;PassengerCount&quot; s_4,[br/]
&amp;nbsp;&amp;nbsp;&amp;nbsp;XSA(&#39;weblogic&#39;.&#39;State Demographics&#39;).&quot;Columns&quot;.&quot;Household Income&quot; s_5,[br/]
&amp;nbsp;&amp;nbsp;&amp;nbsp; XSA(&#39;weblogic&#39;.&#39;State Demographics&#39;).&quot;Columns&quot;.&quot;Population&quot; s_6[br/]
FROM &quot;A - Sample Sales&quot;</sawd:HTML></sawd:htmlView><sawd:htmlView name="HTML 0" duid="a1kq4gp7vs2161ic"><sawd:HTML fmt="html">&lt;img height=180px src=&quot;saw.dll?downloadfile&amp;Path=%2Fshared%2F06.%20Dashboard%20Design%2FExternal%20Sources%2Fpics%2FSA_XL_DS.jpg&quot;/&gt;</sawd:HTML></sawd:htmlView></sawd:dashboardSection><sawd:dashboardSection layoutType="bestFit" name="Section 2" collapsible="false" duid="frrieavhdaro1qt9" showSectionTitle="false"><saw:displayFormat><saw:formatSpec borderPosition="none" wrapText="true"/></saw:displayFormat><sawd:reportView name="Report 2" display="embed" showView="" duid="mpsn0clrmo4i6s05"><saw:caption><saw:text>Mashup of XL and Dataset with SA</saw:text></saw:caption><sawd:reportRef type="catalog" path="/shared/06. Dashboard Design/External Sources/Mashup of XL and Dataset with SA"/></sawd:reportView></sawd:dashboardSection></sawd:dashboardColumn></sawd:dashboardPage>