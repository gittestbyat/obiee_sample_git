SELECT f_1 saw_0, f_2 saw_1, f_3 saw_2 FROM 

	(SELECT a.s_1 f_1, a.s_2 f_2, b.s_2 f_3 FROM 
	
		(SELECT
		replace(&quot;X - Airlines Delay&quot;.&quot;Date&quot;.&quot;Dep Month&quot;,&#39;2010&#39;,&#39;2013&#39;) s_1,
		&quot;X - Airlines Delay&quot;.&quot;Flight Facts&quot;.&quot;# of Flights&quot; s_2
		FROM &quot;X - Airlines Delay&quot;) a,
		
		(SELECT
		&quot;A - Sample Sales&quot;.&quot;Time&quot;.&quot;T02 Per Name Month&quot; s_1,
		&quot;A - Sample Sales&quot;.&quot;Base Facts&quot;.&quot;1- Revenue&quot; s_2
		FROM &quot;A - Sample Sales&quot;) b
	
		[b]WHERE a.s_1 = b.s_1[/b]
	) 
ORDER BY saw_0
