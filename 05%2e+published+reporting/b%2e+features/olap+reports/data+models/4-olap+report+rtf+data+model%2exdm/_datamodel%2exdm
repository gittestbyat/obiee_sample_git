<?xml version = '1.0' encoding = 'utf-8'?>
<dataModel version="2.0" xmlns="http://xmlns.oracle.com/oxp/xmlp" xmlns:xdm="http://xmlns.oracle.com/oxp/xmlp" xmlns:xsd="http://wwww.w3.org/2001/XMLSchema" defaultDataSourceRef="demo">
   <description>
      <![CDATA[Data%20Source:%20OLAP%20(Essbase);%20Structure:%20Concatenated%20flat%20%20XML;%20Features:%20Supports%20multiple%20views%20on%20data%20stored%20in%20Essbase%20or%20other%20OLAP%20sources.%20MDX%20Query%20examples%20(work%20with%20Sample.Basic)]]>
   </description>
   <dataProperties>
      <property name="include_parameters" value="true"/>
      <property name="include_null_Element" value="false"/>
      <property name="include_rowsettag" value="false"/>
      <property name="xml_tag_case" value="as_are"/>
   </dataProperties>
   <dataSets>
      <dataSet name="G1_Gen2Yr-Meas" type="complex">
         <olap dataSourceRef="Essbase">
            <olapQuery>
               <![CDATA[SELECT 
  NON EMPTY {[Measures].[Sales],[Measures].[COGS],[Measures].[Margin],[Measures].[Total Expenses],[Measures].[Profit]} ON COLUMNS,
  NON EMPTY [Year].Generations(2).Members ON ROWS
FROM Sample.Basic]]>
            </olapQuery>
         </olap>
      </dataSet>
      <dataSet name="G2_Gen2Yr-MeasOnRows" type="complex">
         <olap dataSourceRef="Essbase">
            <olapQuery>
               <![CDATA[SELECT 
  NON EMPTY {[Measures].[Sales],[Measures].[COGS],[Measures].[Margin],[Measures].[Total Expenses],[Measures].[Profit]} PROPERTIES GEN_NUMBER ON ROWS,
  NON EMPTY [Year].Generations(2).Members ON COLUMNS
FROM Sample.Basic]]>
            </olapQuery>
         </olap>
      </dataSet>
      <dataSet name="G3_Gen2Yr-Prod-Meas" type="complex">
         <olap dataSourceRef="Essbase">
            <olapQuery>
               <![CDATA[SELECT 
NON EMPTY {[Measures].[Sales],[Measures].[COGS],[Measures].[Margin],[Measures].[Total Expenses],[Measures].[Profit]} ON COLUMNS, NON EMPTY CROSSJOIN([Year].Generations(2).Members, DESCENDANTS(Product)) PROPERTIES MEMBER_ALIAS, GEN_NUMBER, LEVEL_NUMBER ON ROWS
FROM Sample.Basic]]>
            </olapQuery>
         </olap>
      </dataSet>
      <dataSet name="G4_Gen2Yr-Mrkt-Meas" type="complex">
         <olap dataSourceRef="Essbase">
            <olapQuery>
               <![CDATA[SELECT 
  NON EMPTY {[Measures].[Sales],[Measures].[COGS],[Measures].[Margin],[Measures].[Total Expenses],[Measures].[Profit]} ON COLUMNS, NON EMPTY CROSSJOIN([Year].Generations(2).Members, DESCENDANTS(Market)) PROPERTIES MEMBER_ALIAS, GEN_NUMBER, LEVEL_NUMBER ON ROWS
FROM Sample.Basic]]>
            </olapQuery>
         </olap>
      </dataSet>
      <dataSet name="G5_Gen2Mrkt-Prod-Meas" type="complex">
         <olap dataSourceRef="Essbase">
            <olapQuery>
               <![CDATA[WITH 
MEMBER [Measures].[New_Sales] AS 'IIF(IsEmpty(Sales), 0, Sales)',Solve_order=100

MEMBER [Measures].[New_COGS] AS 'IIF(IsEmpty(COGS), 0, COGS)',Solve_order=100

MEMBER [Measures].[New_Margin] AS 'IIF(IsEmpty(Margin), 0, Margin)',Solve_order=200

MEMBER [Measures].[New_Total Expenses] AS 'IIF(IsEmpty([Total Expenses]), 0, [Total Expenses])',Solve_order=200

MEMBER [Measures].[New_Profit] AS 'IIF(IsEmpty(Profit), 0, Profit)',Solve_order=300

SELECT 
NON EMPTY {[Measures].[New_Sales],[Measures].[New_COGS],[Measures].[New_Margin],[Measures].[New_Total Expenses],[Measures].[New_Profit]} ON COLUMNS,
  NON EMPTY 
CROSSJOIN(
          [Market].generations(2).members, 
          EXCEPT(DESCENDANTS(Product),CHILDREN(Diet))
          )
PROPERTIES  MEMBER_ALIAS, LEVEL_NUMBER, GEN_NUMBER ON ROWS
FROM Sample.Basic]]>
            </olapQuery>
         </olap>
      </dataSet>
      <dataSet name="G6_Gen2Yr-Prod-MeasOnPage" type="complex">
         <olap dataSourceRef="Essbase">
            <olapQuery>
               <![CDATA[SELECT 
NON EMPTY {[Measures].[Sales],[Measures].[COGS],[Measures].[Margin],[Measures].[Total Expenses],[Measures].[Profit]} ON PAGES,
NON EMPTY [Year].generations(2).members on COLUMNS, 
NON EMPTY DESCENDANTS(Product) PROPERTIES MEMBER_ALIAS, LEVEL_NUMBER, GEN_NUMBER on ROWS
FROM Sample.Basic]]>
            </olapQuery>
         </olap>
      </dataSet>
   </dataSets>
   <output uniqueRowName="false">
