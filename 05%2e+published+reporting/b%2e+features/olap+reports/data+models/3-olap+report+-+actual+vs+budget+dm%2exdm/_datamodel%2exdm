   <dataSets>
      <dataSet name="G_1:Actual Vs Budget by Qtr 1" type="complex">
         <olap dataSourceRef="Essbase">
            <olapQuery>
               <![CDATA[SELECT NON EMPTY CROSSJOIN(
{[Measures].[Sales],[Measures].[COGS],[Measures].[Margin],[Measures].[Total Expenses],[Measures].[Profit]}, {[Scenario].[Actual], [Scenario].[Budget]}) on COLUMNS, NON EMPTY DESCENDANTS(Year) on ROWS
From Sample.Basic]]>
            </olapQuery>
         </olap>
      </dataSet>
      <dataSet name="G_2:Actual Vs Budget by Qtr 2" type="complex">
         <olap dataSourceRef="Essbase">
            <olapQuery>
               <![CDATA[SELECT NON EMPTY CROSSJOIN(
{[Measures].[Sales],[Measures].[COGS],[Measures].[Margin],[Measures].[Total Expenses],[Measures].[Profit]}, {[Scenario].[Actual], [Scenario].[Budget]}) on ROWS, NON EMPTY DESCENDANTS(Year) on COLUMNS
From Sample.Basic]]>
            </olapQuery>
         </olap>
      </dataSet>
      <dataSet name="G_3:Actual Vs Budget 3" type="complex">
         <olap dataSourceRef="Essbase">
            <olapQuery>
               <![CDATA[SELECT NON EMPTY
{[Measures].[Sales],[Measures].[COGS],[Measures].[Margin],[Measures].[Total Expenses],[Measures].[Profit]} on ROWS,
NON EMPTY CROSSJOIN({[Scenario].[Actual], [Scenario].[Budget]},[Year].Generations(2).Members) ON COLUMNS
From Sample.Basic]]>
            </olapQuery>
         </olap>
      </dataSet>
      <dataSet name="G_4:Meas-Scen-Gen2Year" type="complex">
         <olap dataSourceRef="Essbase">
            <olapQuery>
               <![CDATA[SELECT NON EMPTY
{[Measures].[Sales],[Measures].[COGS],[Measures].[Margin],[Measures].[Total Expenses],[Measures].[Profit]} on Axis(0),
NON EMPTY CROSSJOIN({[Scenario].[Actual], [Scenario].[Budget]},[Year].Generations(2).Members) ON Axis(1)
From Sample.Basic]]>
            </olapQuery>
         </olap>
      </dataSet>
   </dataSets>
