<?xml version = '1.0' encoding = 'utf-8'?>
<dataModel version="2.0" xmlns="http://xmlns.oracle.com/oxp/xmlp" xmlns:xdm="http://xmlns.oracle.com/oxp/xmlp" xmlns:xsd="http://wwww.w3.org/2001/XMLSchema" defaultDataSourceRef="demo">
   <description>
      <![CDATA[Data%20Source:%20OLAP%20(Essbase);%20Structure:%20Concatenated%20flat%20%20XML;%20Features:%20Supports%20multiple%20views%20on%20data%20stored%20in%20Essbase%20or%20other%20OLAP%20sources.%20MDX%20Query%20examples%20(work%20with%20Sample.Basic)]]>
   </description>
   <dataProperties>
      <property name="include_parameters" value="true"/>
      <property name="include_null_Element" value="false"/>
      <property name="include_rowsettag" value="false"/>
      <property name="xml_tag_case" value="upper"/>
   </dataProperties>
   <dataSets>
      <dataSet name="G_1 MeasOnCol-Yr" type="complex">
         <olap dataSourceRef="Essbase">
            <olapQuery>
               <![CDATA[SELECT 
  NON EMPTY {[Measures].[Sales],[Measures].[COGS],[Measures].[Margin],[Measures].[Total Expenses],[Measures].[Profit]} ON COLUMNS,
  NON EMPTY [Year].Generations(2).Members ON ROWS
FROM Sample.Basic]]>
            </olapQuery>
         </olap>
      </dataSet>
      <dataSet name="G_2 MeasOnRows-Yr" type="complex">
         <olap dataSourceRef="Essbase">
            <olapQuery>
               <![CDATA[SELECT 
  NON EMPTY {[Measures].[Sales],[Measures].[COGS],[Measures].[Margin],[Measures].[Total Expenses],[Measures].[Profit]} ON ROWS,
  NON EMPTY [Year].Generations(2).Members ON COLUMNS
FROM Sample.Basic]]>
            </olapQuery>
         </olap>
      </dataSet>
      <dataSet name="G_3 Meas-Prod-Yr" type="complex">
         <olap dataSourceRef="Essbase">
            <olapQuery>
               <![CDATA[SELECT 
  NON EMPTY {[Measures].[Sales],[Measures].[COGS],[Measures].[Margin],[Measures].[Total Expenses],[Measures].[Profit]} ON COLUMNS, NON EMPTY CROSSJOIN([Year].Generations(2).Members, {EXCEPT(DESCENDANTS(Product),CHILDREN([Diet]))}) PROPERTIES MEMBER_ALIAS, GEN_NUMBER, LEVEL_NUMBER ON ROWS
FROM Sample.Basic]]>
            </olapQuery>
         </olap>
      </dataSet>
      <dataSet name="G_4 Meas-Mrkt-Yr" type="complex">
         <olap dataSourceRef="Essbase">
            <olapQuery>
               <![CDATA[SELECT 
  NON EMPTY {[Measures].[Sales],[Measures].[COGS],[Measures].[Margin],[Measures].[Total Expenses],[Measures].[Profit]} ON COLUMNS, NON EMPTY CROSSJOIN([Year].Generations(2).Members, DESCENDANTS(Market)) PROPERTIES MEMBER_ALIAS, GEN_NUMBER, LEVEL_NUMBER ON ROWS
FROM Sample.Basic]]>
            </olapQuery>
         </olap>
      </dataSet>
      <dataSet name="G_5 Meas-Prod-Mrkt-Yr" type="complex">
         <olap dataSourceRef="Essbase">
            <olapQuery>
               <![CDATA[WITH 
MEMBER [Measures].[New_Sales] AS 'IIF(IsEmpty(Sales), 0, Sales)',Solve_order=100
SELECT 
NON EMPTY {[Measures].[New_Sales],[Measures].[COGS],[Measures].[Margin],[Measures].[Total Expenses],[Measures].[Profit]} ON COLUMNS,
  NON EMPTY 
CROSSJOIN(
          [Market].generations(2).members, 
          DESCENDANTS(Product)
          )
PROPERTIES  MEMBER_ALIAS, LEVEL_NUMBER, GEN_NUMBER ON ROWS
FROM Sample.Basic]]>
            </olapQuery>
         </olap>
      </dataSet>
      <dataSet name="G_6 MeasOnRows-Prod-Yr" type="complex">
         <olap dataSourceRef="Essbase">
            <olapQuery>
               <![CDATA[SELECT 
  NON EMPTY CROSSJOIN (
                      {[Measures].[Sales],[Measures].[COGS],[Measures].[Margin],[Measures].[Total Expenses],[Measures].[Profit]}, 
                      [Year].Generations(2).Members
                      ) ON ROWS, 
  NON EMPTY EXCEPT(DESCENDANTS(Product), UNION({[Diet]},CHILDREN(Diet))) ON COLUMNS
FROM Sample.Basic]]>
            </olapQuery>
         </olap>
      </dataSet>
      <dataSet name="G_7 Meas-Product-Time" type="complex">
         <olap dataSourceRef="Essbase">
            <olapQuery>
               <![CDATA[SELECT 
NON EMPTY {[Measures].[Sales],[Measures].[COGS],[Measures].[Margin],[Measures].[Total Expenses],[Measures].[Profit]} ON COLUMNS,
  NON EMPTY 
CROSSJOIN(
          [Product].generations(2).members, 
          DESCENDANTS(Year)
          )
PROPERTIES  MEMBER_ALIAS, LEVEL_NUMBER, GEN_NUMBER ON ROWS
FROM Sample.Basic]]>
            </olapQuery>
         </olap>
      </dataSet>
   </dataSets>
