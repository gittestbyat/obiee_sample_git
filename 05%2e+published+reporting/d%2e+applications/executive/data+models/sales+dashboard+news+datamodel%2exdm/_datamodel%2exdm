   <dataSets>
      <dataSet name="SalesData" type="complex">
         <sql dataSourceRef="demo">
            <![CDATA[/* For JavaDB 
SELECT customers_view.city, customers_view.state_province,customers_view.customer_id,orders_view.order_id,
       orders_view.order_date, orders_view.order_mode,
       orders_view.order_status, orders_view.order_total,
       (employees.first_name || ' ' || employees.last_name) salesrep,
       (orders_view.order_total * employees.commission_pct) salecomm,
       (customers_view.cust_first_name || ' ' || customers_view.cust_last_name
       ) customer_name,
       employees.employee_id, customers_view.customer_id as customer_id_1,
       customers_view.street_address, customers_view.postal_code,
       customers_view.credit_limit, customers_view.cust_email,
       year(orders_view.order_date) AS order_year,
       product_categories.category_name AS prod_cat
  FROM customers_view,
       orders_view,
       employees,
       order_items,
       product_information,
       product_categories
 WHERE (    (customers_view.customer_id = orders_view.customer_id)
        AND (orders_view.sales_rep_id = employees.employee_id)
        AND (orders_view.order_id = order_items.order_id)
        AND (order_items.product_id = product_information.product_id)
        AND (product_information.category_id = product_categories.category_id)
       )
*/
/* Oracle Database */
SELECT customers_view.city, customers_view.state_province,customers_view.customer_id,orders_view.order_id,
       orders_view.order_date, orders_view.order_mode,
       orders_view.order_status, orders_view.order_total,
       decode((employees.first_name || ' ' || employees.last_name),' ','Online',(employees.first_name || ' ' || employees.last_name)) salesrep,
       (orders_view.order_total * employees.commission_pct) salecomm,
       (customers_view.cust_first_name || ' ' || customers_view.cust_last_name
       ) customer_name,
       employees.employee_id,
       customers_view.street_address, customers_view.postal_code,
       customers_view.credit_limit, customers_view.cust_email,
       TO_CHAR(orders_view.order_date, 'YYYY') AS order_year,
       product_information.product_name,
              (DECODE (category_id,
                11, 'Monitors',
                12, 'Printers',
                13, 'Drives',
                14, 'Memory',
                15, 'CPUs',
                19, 'Sundries',
                21, 'Software',
                22, 'Software',
                24, 'Software',
                25, 'Software',
                29, 'Software',
                'Office Furniture'
               )
       ) prod_cat
  FROM customers_view,
       orders_view,
       hr.employees,
       order_items,
       product_information
 WHERE (    (customers_view.customer_id = orders_view.customer_id)
        AND (orders_view.sales_rep_id = employees.employee_id)
        AND (orders_view.order_id = order_items.order_id)
        AND (order_items.product_id = product_information.product_id)
       )]]>
         </sql>
      </dataSet>
      <dataSet name="news" type="simple">
         <http method="GET">
            <url>http://rss.news.yahoo.com/rss/topstories</url>
         </http>
      </dataSet>
      <dataSet name="newsny" type="simple">
         <http method="GET">
            <url>http://www.nytimes.com/services/xml/rss/userland/Business.xml</url>
         </http>
      </dataSet>
      <dataSet name="weather" type="simple">
         <http method="GET">
            <url>http://www.weather.gov/data/current_obs/KSFO.xml</url>
         </http>
      </dataSet>
   </dataSets>
