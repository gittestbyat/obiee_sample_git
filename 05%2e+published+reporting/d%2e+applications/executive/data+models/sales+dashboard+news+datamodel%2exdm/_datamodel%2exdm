   <description>
      <![CDATA[Data%20Sources:%20SQL%20query%20against%20customers_view,%20orders_view,%20hr.employees,%20order_items,%20product_information.%20%0AHTTP%20(RSS%20feeds):%20(1)%20http://rss.news.yahoo.com/rss/topstories,%20(2)%20http://www.nytimes.com/services/xml/rss/userland/Business.xml,%20(3)%20http://www.weather.gov/data/current_obs/KSFO.xml.]]>
   </description>
   <dataProperties>
      <property name="include_parameters" value="true"/>
