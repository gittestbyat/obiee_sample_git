   <valueSets>
      <valueSet id="YEAR_LOV">
         <sql dataSourceRef="Oracle BI EE">
            <![CDATA[select	 "Time"."T05 Per Name Year" as "T05 Per Name Year" 
 from	"A - Sample Sales"."Time" "Time" 
 where 	 "Time"."T05 Per Name Year" IN ('2008', '2009', '2010')
order by "Time"."T05 Per Name Year" ASC]]>
         </sql>
      </valueSet>
   </valueSets>
   <parameters>
      <parameter name="P_YEAR" dataType="xsd:string">
         <select label="Calendar Year" valueSet="YEAR_LOV" multiple="false" all="false" allValue="null" refreshParameters="false"/>
      </parameter>
   </parameters>
