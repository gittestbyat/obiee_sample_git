               <element name="Project_Name" value="Project Name" label="Project Name" dataType="xsd:string" breakOrder="" fieldOrder="10"/>
               <element name="Total_Amount" value="${G_1.Rate*G_1.PJR_Hours}" label="Total_Amount" dataType="xsd:double" breakOrder="None" fieldOrder="0"/>
               <element name="Status" value="${DECODE(G_1.Project_ID, 'Not Assigned', 'Not Assigned' , 'Assigned')}" label="Status" dataType="xsd:string" breakOrder="None" fieldOrder="0"/>
               <element name="Week_Short" value="${FORMAT_DATE(G_1.Week,'dd-MMM')}" label="Week_Short" dataType="xsd:string" breakOrder="None" fieldOrder="0"/>
               <element name="Week_Number" value="Week_Number" label="Week_Number" dataType="xsd:double" breakOrder="" fieldOrder="4"/>
               <element name="Forecast_Hours" value="${NVL(G_1.PJR_Hours, DECODE(G_1.Project_ID, 'Not Assigned', 0 , 40))}" label="Forecast_Hours" dataType="xsd:integer" breakOrder="None" fieldOrder="0"/>
               <element name="Expected_Hours" value="Expected_Hours" label="Expected_Hours" dataType="xsd:double" breakOrder="" fieldOrder="6"/>
               <element name="Inventory_Name" value="Inventory Name" label="Inventory Name" dataType="xsd:string" breakOrder="" fieldOrder="11"/>
