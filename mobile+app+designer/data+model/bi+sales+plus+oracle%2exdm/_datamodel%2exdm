      <dataSet name="Sales" type="complex">
         <sql>
            <![CDATA[SELECT
   "A - Sample Sales"."Products"."P1  Product" as "Product",
   "A - Sample Sales"."Products"."P2  Product Type" as "Product Type",
   "A - Sample Sales"."Products"."P3  LOB" as "LOB",
   "A - Sample Sales"."Products"."P4  Brand" as "Brand",
   "A - Sample Sales"."Time"."T02 Per Name Month" as "Per Name Month",
   "A - Sample Sales"."Time"."T03 Per Name Qtr" as "Per Name Qtr",
   "A - Sample Sales"."Time"."T05 Per Name Year" as "Per Name Year",
   "A - Sample Sales"."Base Facts"."1- Revenue" as "Revenue",
   "A - Sample Sales"."Base Facts"."2- Billed Quantity" as "Billed Quantity",
   "A - Sample Sales"."Base Facts"."5- Target Revenue" as "Target Revenue",
   "A - Sample Sales"."Base Facts"."6- Target Quantity" as "Target Quantity",
   "A - Sample Sales"."Time Series"."121  Period Ago Rev" as "Period Ago Revenue",
   "A - Sample Sales"."Time Series"."122  Rev Var to Period Ago" as "Revenue Variation to Last Period",
   "A - Sample Sales"."Time Series"."127  Mth Ago Rev  (Fix Time Lvl)" as "Month Ago Revenue",
   "A - Sample Sales"."Time Series"."128  Qtr Ago Rev  (Fix Time Lvl)" as "Quarter Ago Revenue",
   "A - Sample Sales"."Time Series"."129  Year Ago Rev  (Fix Time Lvl)" as "Year Ago Revenue"
FROM "A - Sample Sales"
ORDER BY 1, 5 ASC NULLS LAST, 4 ASC NULLS LAST, 2 ASC NULLS LAST, 3 ASC NULLS LAST, 8 ASC NULLS LAST, 7 ASC NULLS LAST, 6 ASC NULLS LAST]]>
         </sql>
      </dataSet>
      <dataSet name="Oracle" type="complex">
