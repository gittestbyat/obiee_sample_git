      <dataSet name="Executives" type="complex">
         <sql dataSourceRef="BISAMPLE">
            <![CDATA[select	 "ORACLE_EXECUTIVES"."ID" as "ID",
	 "ORACLE_EXECUTIVES"."EXECTIVE_NAME" as "EXECTIVE_NAME",
	 "ORACLE_EXECUTIVES"."TITLE" as "TITLE",
	 "ORACLE_EXECUTIVES"."BIOGRAPHY" as "BIOGRAPHY",
	 "ORACLE_EXECUTIVES"."FACE_URL" as "FACE_URL" 
 from	"BISAMPLE_OTHER"."ORACLE_EXECUTIVES" "ORACLE_EXECUTIVES"
order by ID]]>
         </sql>
      </dataSet>
   </dataSets>
