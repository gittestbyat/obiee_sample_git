               <element name="GEO_REGION" value="GEO_REGION" label="GEO_REGION" dataType="xsd:string" breakOrder="" fieldOrder="8"/>
               <element name="ACTIVITY_TYPE_CODE" value="ACTIVITY_TYPE_CODE" label="ACTIVITY_TYPE_CODE" dataType="xsd:string" breakOrder="" fieldOrder="9"/>
