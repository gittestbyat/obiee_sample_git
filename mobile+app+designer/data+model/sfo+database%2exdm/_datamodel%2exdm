   <dataSets>
      <dataSet name="Data" type="complex">
         <sql dataSourceRef="BISAMPLE">
            <![CDATA[select a.id, b.year current_year, a.year prev_year, a.month, a.operating_airline, a.operating_airline_iata_code, a.geo_summary, a.geo_region, a.activity_type_code, a.price_category_code, 
a.terminal, a.boarding_area, nvl(b.passenger_count, 0) current_year_count, nvl(a.passenger_count,0) prev_year_count, nvl(b.passenger_count, 0) - nvl(a.passenger_count,0) change_count
from BISAMPLE_OTHER.SFO_DATA a, BISAMPLE_OTHER.SFO_DATA b
where a.year = b.year-1
and a.month = b.month
and a.operating_airline (+) = b.operating_airline
and a.operating_airline_iata_code (+)  = b.operating_airline_iata_code
and a.geo_summary (+)  = b.geo_summary
and a.geo_region (+)  = b.geo_region
and a.activity_type_code (+)  = b.activity_type_code 
and a.price_category_code (+)  = b.price_category_code
and a.terminal (+)  = b.terminal
and a.boarding_area (+)  = b.boarding_area
order by current_year, month]]>
         </sql>
      </dataSet>
   </dataSets>
   <output rootName="DATA_DS" uniqueRowName="false">
