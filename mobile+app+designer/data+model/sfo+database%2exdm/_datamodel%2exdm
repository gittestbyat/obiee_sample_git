               <element name="BOARDING_AREA" value="BOARDING_AREA" label="BOARDING_AREA" dataType="xsd:string" breakOrder="" fieldOrder="12"/>
               <element name="CURRENT_YEAR_COUNT" value="CURRENT_YEAR_COUNT" label="CURRENT_YEAR_COUNT" dataType="xsd:double" breakOrder="" fieldOrder="13"/>
