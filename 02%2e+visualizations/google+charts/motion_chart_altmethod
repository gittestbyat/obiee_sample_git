     function drawMotionChart() {
if(document.URL.indexOf(&quot;saw.dll?Answers&quot;) == -1) {
        var data = new google.visualization.DataTable();
        data.addColumn(&#39;string&#39;, &#39;Country&#39;);
        data.addColumn(&#39;date&#39;, &#39;Date&#39;);
        data.addColumn(&#39;number&#39;, &#39;Revenue&#39;);
        data.addColumn(&#39;number&#39;, &#39;Costs&#39;);
        data.addColumn(&#39;string&#39;, &#39;Brand&#39;);
        data.addRows(myMCArray);

        var chart = new google.visualization.MotionChart(document.getElementById(&#39;div_MotionChart&#39;));
        chart.draw(data, {width: 600, height:300});
}
      }
