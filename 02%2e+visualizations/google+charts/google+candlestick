function drawCandlestick() {
if(document.URL.indexOf(&quot;saw.dll?Answers&quot;) == -1)  {
var dataTable = google.visualization.arrayToDataTable(myCandlestickArray, true);
var chart = new google.visualization.CandlestickChart(document.getElementById(&#39;div_Candlestick&#39;));        
chart.draw(dataTable, {legend:&#39;none&#39;, width:700, height:400});  
}
}    

if(document.URL.indexOf(&quot;saw.dll?Answers&quot;) == -1) 
google.setOnLoadCallback(drawCandlestick);  
