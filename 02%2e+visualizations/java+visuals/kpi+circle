&lt;script&gt;

  var canvas = document.getElementById(&#39;myCanvas&#39;);
  var context = canvas.getContext(&#39;2d&#39;);

  var params = [];

  params[&#39;canvasWidth&#39;] = canvas.width;
  params[&#39;canvasHeight&#39;] = canvas.height;


  function drawKPI(label, value, color, name, count, rows) {

    params[&#39;middleY&#39;] = (params[&#39;canvasHeight&#39;] / 2.5) + 50;
    params[&#39;middleX&#39;] = params[&#39;canvasWidth&#39;] / rows * (count - 0.5);

    outerRadius = params[&#39;canvasHeight&#39;] / @{outerRadius}{5} ;

    innerRadius = @{innerRadius}{20};

    context.beginPath();
    context.arc(params[&#39;middleX&#39;], params[&#39;middleY&#39;], outerRadius, 0, 2 * Math.PI, false);
    context.fillStyle = &#39;lightgrey&#39;;
    context.fill();

    rad= innerRadius + (outerRadius - innerRadius)/2;
    width= outerRadius - innerRadius;
    context.beginPath();
    context.strokeStyle= color;
    context.lineWidth=width;

    context.arc(params[&#39;middleX&#39;], params[&#39;middleY&#39;],rad ,(Math.PI/180)*0,(Math.PI/180)*(360 / 100*value),false);
    context.stroke();
    context.closePath();

    context.beginPath();
    context.arc(params[&#39;middleX&#39;], params[&#39;middleY&#39;], innerRadius, 0, 2 * Math.PI, false);
    context.fillStyle = &#39;white&#39;;
    context.fill();

    context.font = &#39;20pt Calibri&#39;;
    context.fillStyle = &#39;black&#39;;
    context.textAlign = &#39;center&#39;;
    context.textBaseline = &#39;middle&#39;;
    context.fillText(label, params[&#39;middleX&#39;], params[&#39;middleY&#39;]);

    context.font = &#39;15pt Calibri&#39;;
    context.fillStyle = &#39;red&#39;;
    context.textAlign = &#39;center&#39;;
    context.fillText(name, params[&#39;middleX&#39;], 20);
  }</saw:text></saw:caption></saw:prefix><saw:narrative><saw:caption fmt="html"><saw:text>drawKPI(&#39;@{c91bb7c48bb60947d}&#39;, &#39;@{c5e9343adb005ba0e}&#39;, &#39;@{c02a4dfa58bdc4767}&#39;, &#39;@{c2eac18888864808b}&#39;, &#39;@{c6e4924389c53d5dc}&#39;, &#39;@{c1c24f9e145ff85fd}&#39;);</saw:text></saw:caption></saw:narrative><saw:postfix><saw:caption fmt="html"><saw:text>&lt;/script&gt;</saw:text></saw:caption></saw:postfix></saw:view><saw:view xsi:type="saw:tableView" name="tableView!2" scrollingEnabled="true"><saw:edges><saw:edge axis="page" showColumnHeader="true"/><saw:edge axis="section"/><saw:edge axis="row" showColumnHeader="true"><saw:edgeLayers><saw:edgeLayer type="column" columnID="c2eac18888864808b"/><saw:edgeLayer type="column" columnID="c91bb7c48bb60947d"/><saw:edgeLayer type="column" columnID="c5e9343adb005ba0e"/><saw:edgeLayer type="column" columnID="c02a4dfa58bdc4767"/><saw:edgeLayer type="column" columnID="c6e4924389c53d5dc"/><saw:edgeLayer type="column" columnID="c1c24f9e145ff85fd"/></saw:edgeLayers></saw:edge><saw:edge axis="column" showColumnHeader="rollover"/></saw:edges></saw:view></saw:views></saw:report>