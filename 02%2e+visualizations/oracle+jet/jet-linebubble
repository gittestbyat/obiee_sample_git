&lt;script&gt;
document.getElementById(&quot;css&quot;).href = require.toUrl(&quot;ojs&quot;)+&quot;.css&quot;;
$( &quot;html&quot; ).css( &quot;font-size&quot;, 14);

require([&#39;ojs/ojcore&#39;, &#39;knockout&#39;, &#39;jquery&#39;, &#39;ojs/ojknockout&#39;, &#39;ojs/ojbutton&#39;, &#39;ojs/ojchart&#39;],
function(oj, ko, $)
{   
    function ChartModel() {
        var self = this;
        self.lineTypeValue = ko.observable(&#39;stepped&#39;);
        
        /* chart data 
        var bubbleSeries = [{name: &#39;Series 1&#39;, items: [{x: 5, y: 0, z: 1}, {x: 0, y: 10, z: 1.5}, {x: -15, y: 0, z: 2}, {x: 0, y: -20, z: 3}, {x: 25, y: 0, z: 4}, {x: 0, y: 30, z: 5}, {x: -40, y: 0, z: 7}]}];
        var bubbleGroups = [&quot;2008&quot;, &quot;2009&quot;, &quot;2010&quot;, &quot;2011&quot;, &quot;2012&quot;, &quot;2013&quot;, &quot;2014&quot;];
		*/
        
        this.bubbleSeriesValue = ko.observableArray(bubbleSeries);
        this.bubbleGroupsValue = ko.observableArray(bubbleGroups);
        
        self.formatRadios = ko.observableArray([
            {id: &#39;straight&#39;, label: &#39;straight&#39;},
            {id: &#39;curved&#39;, label: &#39;curved&#39;},
            {id: &#39;stepped&#39;, label: &#39;stepped&#39;},
            {id: &#39;segmented&#39;, label: &#39;segmented&#39;},
            {id: &#39;none&#39;, label: &#39;none&#39;},
        ]);
    }
    
    var chartModel = new ChartModel();
    
    $(document).ready(
	function(){
            ko.applyBindings(chartModel, document.getElementById(&#39;chart-container&#39;));
	}
    );
});	
&lt;/script&gt;

&lt;div id=&#39;chart-container&#39;&gt;
